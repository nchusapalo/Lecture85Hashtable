<Window x:Class="Lecture85Hashtable.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lecture85Hashtable"
        mc:Ignorable="d"
        Title="Hashtable" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtKey" HorizontalAlignment="Left" Height="23" Margin="197,113,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="428" FontWeight="Bold"/>
        <TextBox x:Name="txtValue" HorizontalAlignment="Left" Height="23" Margin="197,160,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="428" FontWeight="Bold"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Margin="197,210,0,0" VerticalAlignment="Top" Width="75" FontWeight="Bold" Click="btnAdd_Click"/>
        <Button x:Name="btnRemove" Content="Remove" HorizontalAlignment="Left" Margin="370,210,0,0" VerticalAlignment="Top" Width="75" FontWeight="Bold" Click="btnRemove_Click"/>
        <Button x:Name="btnShowAll" Content="Show All" HorizontalAlignment="Left" Margin="550,210,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.133,0.4" FontWeight="Bold" Click="btnShowAll_Click"/>

    </Grid>
</Window>
